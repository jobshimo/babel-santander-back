datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Candidate {
  id                Int      @id @default(autoincrement())
  name              String
  surname           String
  seniority         String
  yearsOfExperience Int
  availability      Boolean
  createdAt         DateTime @default(now())
}
